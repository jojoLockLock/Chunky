{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "autoprefixer": null,
  "proxy": {
    "/api":{
      "target":"http://127.0.0.1:9090/",
      "changeOrigin":true,
      "pathRewrite":{"^/api":""}
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",["import", { "libraryName": "antd", "style": "css" }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}

{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "autoprefixer": null,
  "proxy": {
    "/api":{
      "target":"http://127.0.0.1:3000/",
      "changeOrigin":true
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "theme":{
      "primary-color": "#3db8c1",
      "link-color": "#3db8c1",
      "border-radius-base": "2px"
  }
}
